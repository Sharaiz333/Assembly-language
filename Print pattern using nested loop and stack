.MODEL SMALL
.STACK 100H

.DATA
newline DB 0DH, 0AH, '$'  ; Newline characters (Carriage return, Line feed)

.CODE
START:
    ; Initialize data segment
    MOV AX, @DATA
    MOV DS, AX

    MOV CX, 1         ; Outer loop counter (row number)
    
OuterLoop:
    PUSH CX           ; Save the current row number (for restoring after inner loop)
    
    MOV BX, CX        ; Move row number to BX (this is the number to print)
    MOV DX, CX        ; Inner loop counter (number of times to print the row number)

InnerLoop:
    ; Print the current number (BX contains the current number)
    PUSH DX           ; Save the inner loop counter
    MOV DL, BL        ; Move the number to DL
    ADD DL, '0'       ; Convert the number to ASCII
    MOV AH, 02H       ; DOS interrupt to print a character
    INT 21H

    POP DX            ; Restore the inner loop counter
    DEC DX            ; Decrease inner loop counter
    JNZ InnerLoop     ; Repeat inner loop if DX != 0

    ; Print newline after each row
    LEA DX, newline
    MOV AH, 09H       ; DOS interrupt to print string
    INT 21H

    POP CX            ; Restore the outer loop counter
    INC CX            ; Move to the next row
    CMP CX, 6         ; Check if we have printed 5 rows
    JNE OuterLoop     ; If not, repeat the outer loop

    ; Exit the program
    MOV AH, 4CH
    INT 21H

END START